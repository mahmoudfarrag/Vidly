@model Vidly.Models.Movie
@{
    ViewBag.Title = "Detailes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>Release Date: @Model.ReleaseDate.ToLongTimeString()</li>
    <li>Add Date:@Model.DateAdded.ToLongTimeString()</li>
    <li>Number in Stock:@Model.NumberInStock</li>


</ul>

